#include <stdio.h>


void printDNA(int row, int n) {
    if (row >= n) return; 

    int i;

   
    if (row % 8 == 0) {
        printf("*       *\n");
    } else if (row % 8 == 1) {
        printf(" *     * \n");
    } else if (row % 8 == 2) {
        printf("  *   *  \n");
    } else if (row % 8 == 3) {
        printf("   * *   \n");
    } else if (row % 8 == 4) {
        printf("    *    \n");
        
    } else if (row % 8 == 5) {
        printf("   * *   \n");
    } else if (row % 8 == 6) {
        printf("  *   *  \n");
    } else if (row % 8 == 7) {
        printf(" *     * \n");
    }

    
    printDNA(row + 1, n);
}

int main() {
    int n;
    printf("Enter number of rows for DNA pattern: ");
    scanf("%d", &n);

    printDNA(0, n); 

    return 0;
}
