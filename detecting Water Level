#include <stdio.h>

int main() {
   int Water_level;
   printf("enter water level (between 1000-2000):");
   scanf("%d", &Water_level);
   printf("we have entered %d water level\n", Water_level);
   if (Water_level>2000){
       printf("open door");
       
       
   }
   else{
       printf("close door");
   }
